=","lnglat":{"type":"Point","coordinates":[-119.02723,50.14502]}},"createdBy":"N9xDpIFwWTKsQ73FcA5bXw==","_change":{"user":"N9xDpIFwWTKsQ73FcA5bXw==","historyId":"64140c25f0cf0083295f3c90","operation":"updateClimb","seq":0},"updatedAt":"2023-03-17T06:43:49.773Z"},{"_id":"SE+jXPJvR6WkncTVDHPsew==","name":"Full Body Stone","grades":{"yds":"5.8"},"type":{"trad":true,"sport":false,"bouldering":false,"alpine":false,"snow":false,"ice":false,"mixed":false,"aid":false,"tr":false},"gradeContext":"US","fa":"Gary Wolkoff & Lyle Knight, September 2008","content":{"description":"Similar to \"Depths of Despair\" but a tighter squeeze.\n\nAberdeen Wall - Right, #9\nStandard Rack"},"metadata":{"left_right_index":-1,"areaRef":"tz/SzwEMUl6nciAr/BWa3Q==","lnglat":{"type":"Point","coordinates":[-119.02723,50.14502]}},"createdBy":"N9xDpIFwWTKsQ73FcA5bXw==","_change":{"user":"N9xDpIFwWTKsQ73FcA5bXw==","historyId":"64140c26f0cf0083295f3c9a","operation":"updateClimb","seq":0},"updatedAt":"2023-03-17T06:43:50.812Z"},{"_id":"wybmQjWrQNWJ6BwdVExvLg==","name":"Gold Rush","grades":{"yds":"5.10c"},"type":{"trad":true,"sport":false,"bouldering":false,"alpine":false,"snow":false,"ice":false,"mixed":false,"aid":false,"tr":false},"gradeContext":"US","fa":"Lyle Knight, September 2008","content":{"description":"Used to be 10b double crack until 2012, now a single 10c to the last 5m of o/w. The Aberdeen gold rush was 2008/ 2009.\n\nAberdeen Wall - Left, #3\nStandard Rack"},"metadata":{"left_right_index":-1,"areaRef":"tz/SzwEMUl6nciAr/BWa3Q==","lnglat":{"type":"Point","coordinates":[-119.02723,50.14502]}},"createdBy":"N9xDpIFwWTKsQ73FcA5bXw==","_change":{"user":"N9xDpIFwWTKsQ73FcA5bXw==","historyId":"64140c27f0cf0083295f3ca4","operation":"updateClimb","seq":0},"updatedAt":"2023-03-17T06:43:51.643Z"},{"_id":"vuBTYUlyQyi5MtHXc8QFPQ==","name":"Ground Zero","grades":{"yds":"5.11d"},"type":{"trad":true,"sport":false,"bouldering":false,"alpine":false,"snow":false,"ice":false,"mixed":false,"aid":false,"tr":fal